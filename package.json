    "code:store": "barnard59 run -v --otel-traces-exporter=otlp --pipeline=http://opendatabs.github.com/ld-pipeline/code/store pipelines/code.ttl --variable=ENDPOINT_URL=$ENDPOINT_URL --variable=ENDPOINT_USER=$ENDPOINT_USER --variable=ENDPOINT_PASSWORD=$ENDPOINT_PASSWORD",
